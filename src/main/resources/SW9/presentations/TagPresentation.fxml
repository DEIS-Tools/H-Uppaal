<?xml version="1.0" encoding="UTF-8"?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.shape.Path?>
<fx:root xmlns:fx="http://javafx.com/fxml/1"
         xmlns="http://javafx.com/javafx/8.0.76-ea"
         type="StackPane">

    <Path id="shape"/>

    <Label id="label" styleClass="sub-caption" StackPane.alignment="CENTER_LEFT" visible="false"/>

    <JFXTextField id="textField" styleClass="sub-caption" StackPane.alignment="CENTER_LEFT"
                  translateX="4"/>

</fx:root>